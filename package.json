{
  "name": "sprout-auto-attendance",
  "version": "2.0.0",
  "description": "Lazy attendance for lazy developers",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "./node_modules/.bin/serverless deploy --stage prod -v --conceal --color",
    "local": "./node_modules/.bin/serverless offline --stage test --httpPort 3001",
    "test": "npm run lint && npm run coverage",
    "mocha": "./node_modules/.bin/mocha test/**/*.test.ts",
    "lint": "./node_modules/.bin/eslint . --ext .ts --fix",
    "coverage": "nyc npm run mocha"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "google-auth-library": "^7.0.4",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.44",
    "@types/selenium-webdriver": "^4.0.12",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "json-schema-to-ts": "^1.5.0",
